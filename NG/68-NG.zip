				hideNetworkName
					TruthValue,
				vlanID
					INTEGER,
				clientSecuritySeperation
					INTEGER,
				authenticationType
					INTEGER,
				encryption
					INTEGER,
				presharedKey
					OCTET STRING,
			        wepKeyType
					INTEGER,
				wepKeyNo
					INTEGER,
				wepKey1
					OCTET STRING,
				wepKey2
					OCTET STRING,
				wepKey3
					OCTET STRING,
				wepKey4
					OCTET STRING,
				wepPassPhrase
					OCTET STRING,
